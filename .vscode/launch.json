{
    // See https://aka.ms/vscode-docker-dotnet-launch for more info about debugging .NET in VS Code
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Launch (Console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net8.0/PacketCapture.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}
